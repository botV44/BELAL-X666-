# Bot runtime
NODE_ENV=production

# Messenger bot token (from QueenChat or similar)
BOT_TOKEN=your_bot_token_here

# Language fallback (optional override)
DEFAULT_LANG=en

# API keys for external services
QUEENCHAT_API_KEY=your_queenchat_key
CHATGPT_API_KEY=your_openai_key
GEMINI_API_KEY=your_gemini_key

# Logging options
LOG_LEVEL=info
LOG_PATH=logs/
